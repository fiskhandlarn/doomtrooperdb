{% extends 'AppBundle:Search:display.html.twig' %}

{% block main %}

    {% for key, value in cards %}
        <h2>{{ key }}</h2>
        <div class="shortlist" style="-webkit-column-count: 3;-moz-column-count: 3;column-count: 3;">
            {% for i in 0..value|length-1 %}
                {% set card = value[i] %}
                <a href="{{ card.url }}">{{ card.name }}</a><br>
            {% endfor %}
        </div>
    {% endfor %}

{% endblock %}
